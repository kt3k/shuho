{
  "name": "kt3k-shuho",
  "version": "1.0.0",
  "description": "weekly journal of kt3k",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "start": "bulbo",
    "build": "bulbo build",
    "pub": "npm run clean && npm run build && gh-pages -d build -m 'chore: update [skip ci]'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/shuho.git"
  },
  "author": "Yoshiya Hinosawa",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/kt3k/shuho/issues"
  },
  "homepage": "https://github.com/kt3k/shuho#readme",
  "devDependencies": {
    "branch-pipe": "^1.0.1",
    "bulbo": "^7.0.0",
    "gh-pages": "^2.0.0",
    "gulp-data": "^1.3.1",
    "gulp-front-matter": "^1.3.0",
    "gulp-remark": "^6.0.0",
    "gulp-rename": "^1.3.0",
    "husky": "^3.0.0",
    "layout1": "^1.1.0",
    "lint-staged": "^8.0.0",
    "lynt": "^0.5.0",
    "moment": "^2.22.2",
    "nunjucks": "^3.1.6",
    "prettier": "^1.13.6",
    "remark-align": "^1.2.0",
    "remark-attr": "^0.8.0",
    "remark-html": "^9.0.0",
    "remark-slug": "^5.0.0",
    "rimraf": "^2.6.2",
    "vinyl-accumulate": "^1.4.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --no-semi",
      "lynt --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write --prose-wrap always",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
